<%- include('partials/header'); %>

<h1>Compose</h1>

<form  action="/compose" method="post">
  <label for="composePost">Title</label>
  <input type="text" class="form-control" id="composePost" name="composeTitle" Placeholder="write here" autofocus autocomplete="off">
  <br>
  <label for="textArea">Post</label>
  <textarea name="composeBody" class="form-control" id="textArea" rows="6"></textarea>

  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>


<%- include('partials/footer'); %>
